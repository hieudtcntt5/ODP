<?xml version="1.0" encoding="UTF-8"?>
    <?xml-stylesheet type="text/xsl" href="configuration.xsl"?>
    <configuration supports_final="true">
      
      <property>
        <name>base_log_folder</name>
        <value>/usr/odp/1.2.2.0-138/airflow/logs</value>
