        <value>True</value>
        <display-name>Colored Console Log</display-name>
        <description>Custom description for colored_console_log</description>
        
      </property>
      <property>
        <name>colored_log_format</name>
        <value>[%%(blue)s%%(asctime)s%%(reset)s] {%%(blue)s%%(filename)s:%%(reset)s%%(lineno)d} %%(log_color)s%%(levelname)s%%(reset)s - %%(log_color)s%%(message)s%%(reset)s</value>
        <display-name>Colored Log Format</display-name>
        <description>Custom description for colored_log_format</description>
        
      </property>
      <property>
        <name>colored_formatter_class</name>
        <value>airflow.utils.log.colored_log.CustomTTYColoredFormatter</value>
