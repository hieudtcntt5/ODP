        <description>Custom description for task_log_prefix_template</description>
        
          <value-attributes>
            <empty-value-valid>true</empty-value-valid>
          </value-attributes>
      </property>
      <property>
        <name>log_filename_template</name>
        <value>dag_id={{ ti.dag_id }}/run_id={{ ti.run_id }}/task_id={{ ti.task_id }}/{%% if ti.map_index >= 0 %%}map_index={{ ti.map_index }}/{%% endif %%}attempt={{ try_number }}.log</value>
        <display-name>Log Filename Template</display-name>
        <description>Custom description for log_filename_template</description>
        
      </property>
      <property>
        <name>log_processor_filename_template</name>
        <value>{{ filename }}.log</value>
        <display-name>Log Processor Filename Template</display-name>
        <description>Custom description for log_processor_filename_template</description>
        
      </property>
