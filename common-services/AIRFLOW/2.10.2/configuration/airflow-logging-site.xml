          <value-attributes>
            <empty-value-valid>true</empty-value-valid>
          </value-attributes>
      </property>
      <property>
        <name>encrypt_s3_logs</name>
        <value>False</value>
        <display-name>Encrypt S3 Logs</display-name>
        <description>Custom description for encrypt_s3_logs</description>
        
      </property>
      <property>
        <name>logging_level</name>
        <value>INFO</value>
        <display-name>Logging Level</display-name>
        <description>Custom description for logging_level</description>
        
      </property>
      <property>
        <name>celery_logging_level</name>
        <value></value>
        <display-name>Celery Logging Level</display-name>
        <description>Custom description for celery_logging_level</description>
        
          <value-attributes>
            <empty-value-valid>true</empty-value-valid>
